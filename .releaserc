{
  "branches": [
    "master"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/git",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "./artifacts/pong-x86_64-unknown-linux-gnu/pong",
            "label": "Linux x86_64 Binary"
          },
          {
            "path": "./artifacts/pong-aarch64-unknown-linux-gnu/pong",
            "label": "Linux ARM64 Binary"
          },
          {
            "path": "./artifacts/pong-x86_64-apple-darwin/pong",
            "label": "macOS x86_64 Binary"
          },
          {
            "path": "./artifacts/pong-aarch64-apple-darwin/pong",
            "label": "macOS ARM64 Binary"
          }
        ]
      }
    ]
  ]
}